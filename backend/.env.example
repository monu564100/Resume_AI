# Copy this file to .env and fill in your values
PORT=5000
MONGODB_URI=mongodb://127.0.0.1:27017/resume-validator
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d
GEMINI_API_KEY=your_gemini_api_key_here
SKIP_AUTH=true

# Affinda Resume Parser
AFFINDA_API_KEY=your_affinda_api_key_here

# RChilli Resume Parser
RCHILLI_URL=https://rest.rchilli.com/RChilliParser/Rchilli/parseResume
RCHILLI_USERKEY=your_rchilli_userkey_here
RCHILLI_VERSION=3.0.0
RCHILLI_SUBUSERID=your_rchilli_subuserid_here

# RapidAPI (LinkedIn Jobs)
RAPIDAPI_KEY=your_rapidapi_key_here
RAPIDAPI_LINKEDIN_HOST=linkedin-jobs-search.p.rapidapi.com

# SkillRank API
SKILLRANK_API_KEY=your_skillrank_api_key_here
SKILLRANK_BASE_URL=https://api.skillrank.example.com
